{% extends 'base.html.twig' %}

{% block title %}Support index{% endblock %}

{% block body %}
    <h1>Support index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Idsupporttype</th>
                <th>Label</th>
                <th>Dateedited</th>
                <th>Isactive</th>
                <th>Iseditable</th>
                <th>Createdat</th>
                <th>Updatedat</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for support in supports %}
            <tr>
                <td>{{ support.id }}</td>
                <td>{{ support.idsupporttype }}</td>
                <td>{{ support.label }}</td>
                <td>{{ support.dateedited ? support.dateedited|date('Y-m-d') : '' }}</td>
                <td>{{ support.isactive ? 'Yes' : 'No' }}</td>
                <td>{{ support.iseditable ? 'Yes' : 'No' }}</td>
                <td>{{ support.createdat ? support.createdat|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ support.updatedat ? support.updatedat|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('support_show', {'id': support.id}) }}">show</a>
                    <a href="{{ path('support_edit', {'id': support.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="9">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('support_new') }}">Create new</a>
{% endblock %}
