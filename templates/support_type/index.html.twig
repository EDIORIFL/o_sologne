{% extends 'base.html.twig' %}

{% block title %}SupportType index{% endblock %}

{% block body %}
    <h1>SupportType index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Label</th>
                <th>Isactive</th>
                <th>Iseditable</th>
                <th>Createdat</th>
                <th>Updatedat</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for support_type in support_types %}
            <tr>
                <td>{{ support_type.id }}</td>
                <td>{{ support_type.label }}</td>
                <td>{{ support_type.isactive ? 'Yes' : 'No' }}</td>
                <td>{{ support_type.iseditable ? 'Yes' : 'No' }}</td>
                <td>{{ support_type.createdat ? support_type.createdat|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ support_type.updatedat ? support_type.updatedat|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('support_type_show', {'id': support_type.id}) }}">show</a>
                    <a href="{{ path('support_type_edit', {'id': support_type.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="7">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('support_type_new') }}">Create new</a>
{% endblock %}
