{% extends 'base.html.twig' %}

{% block title %}SupportType{% endblock %}

{% block body %}
    <h1>SupportType</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ support_type.id }}</td>
            </tr>
            <tr>
                <th>Label</th>
                <td>{{ support_type.label }}</td>
            </tr>
            <tr>
                <th>Isactive</th>
                <td>{{ support_type.isactive ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Iseditable</th>
                <td>{{ support_type.iseditable ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Createdat</th>
                <td>{{ support_type.createdat ? support_type.createdat|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Updatedat</th>
                <td>{{ support_type.updatedat ? support_type.updatedat|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('support_type_index') }}">back to list</a>

    <a href="{{ path('support_type_edit', {'id': support_type.id}) }}">edit</a>

    {{ include('support_type/_delete_form.html.twig') }}
{% endblock %}
